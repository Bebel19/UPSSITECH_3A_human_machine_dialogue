<rule id="num_ab" scope="public">
<one-of>
     <item> <ruleref uri="#filler_debut_phrase" /> 
            <ruleref uri="grammaire_nombre_v3.grxml#nombre" />
                  
   [cite_start]<tag> out.nb = rules.nombre.nb ; out.text = meta.nombre.text </tag> [cite: 447]
     </item>
