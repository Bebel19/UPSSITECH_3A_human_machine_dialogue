<rule id="filler_debut_phrase">
      <one-of>
              <item> c est  le </item>
              <item> mon numero est le </item>
              <item> le </item>
              <item> je suis l abonne numero </item>
              [cite_start]<item>  [cite: 449]
je suis le </item>
      </one-of>
</rule>
</grammar>